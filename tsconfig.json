{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "preserve",
    "module": "es6",
    "target": "es5",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false
  },
  "include": ["src"]
}
